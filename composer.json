{
    "name": "ajvolin/channels-buddy-source-provider",
    "description": "A package for implementing channel sources for Channels Buddy",
    "keywords": [
        "Channels",
        "Channels Buddy"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Volin",
            "email": "trome.auay@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^2.40.0"
    },
    "autoload": {
        "psr-4": {
            "ChannelsBuddy\\SourceProvider\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChannelsBuddy\\SourceProvider\\ChannelsBuddySourceServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
